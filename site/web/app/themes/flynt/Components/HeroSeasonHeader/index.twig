<div class="flyntComponent" is="flynt-hero-season-header">
  <div class="season-header">
    <div class="season-header__wrapper">
      <div class="season-header__name">
        <h1>{{ post.year }}</h1>
        <span>{{ post.title }}</span>
        {% if post.youtube_video %}
          <a href="{{ post.youtube_video }}" target="_blank">Video</a>
        {% endif %}
      </div>
      <div class="season-header__image">
        <img src="{{ post.group_photo  }}" alt="">
      </div>
    </div>
  </div>
</div>
